//사용자로부터 정수 두개 a,b를 입력받아 a이상 b이하의 정수형 난수를 생성해 출력하시오

import java.util.Scanner;

class Test01
{
	public static void main(String [] args)
	{
		int a,b;
		Scanner sc = new Scanner(System.in);
		
		System.out.print("정수 1 : ");
		a = sc.nextInt();
		System.out.print("정수 2 : ");
		b = sc.nextInt();
		
		System.out.printf("%d이상 %d이하의 랜덤넘버 : %d\n", a, b, (int)(Math.random()*(b-a+1)+a) );
	}
}
